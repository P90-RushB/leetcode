public class Solution {
    public int MaxSubArray(int[] nums) {
    //只需要记录最大的和，而不需要记录使和最大的子数组
        int n=nums.Length;
    int[] temp=new int[n];
        temp[0]=nums[0];
        int max=nums[0];
        for(int i=1;i<n;i++)
        {
            temp[i]=nums[i]+(temp[i-1]>0?temp[i-1]:0);
            max=max>temp[i]?max:temp[i];
        }
        return max;
    }
}
//新建一个temp数组，在遍历nums数组时，对每个i，temp[i]表示nums数组数到i时最大子数组的和（若temp[i]为负数，那temp[i]就为nums[i]自身，否则，
//temp[i]就是temp[i-1]+nums[i]），然后用temp[i]更新max。
